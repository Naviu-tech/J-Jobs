---
import BaseLayout from "@/layouts/BaseLayout.astro";
import HeaderOne from "@/components/headers/HeaderOne.astro";
import FeatureOne from "@/components/features/FeatureOne.astro";
import FeatureThree from "@/components/features/FeatureThree.astro";
import FeatureFour from "@/components/features/FeatureFour.astro";
import TestimonialTwo from "@/components/testimonials/TestimonialTwo.astro";
import PricingOne from "@/components/pricing/PricingOne.astro";
import FaqOne from "@/components/faqs/FaqOne.astro";
import CtaOne from "@/components/cta/CtaOne.astro";
import CompanyEntries from "@/components/company/CompanyEntries.astro";
import CtaIndeed from "@/components/cta/CtaIndeed.astro";

const pageTitle = "Jake's Jobs | The #1 Platform for Hiring Qualified Electricians";
const pageDescription = "Discover roles at innovative companies. Our recruiters connect talented professionals with opportunities that match their skills and aspirations.";
---

<BaseLayout title={pageTitle} description={pageDescription}>
  <Fragment slot="head">
    <link rel="canonical" href={new URL(Astro.url.pathname, Astro.site).toString()} />
  </Fragment>

  <HeaderOne />
  <TestimonialTwo />
  <CompanyEntries />
  <CtaIndeed />
  <FeatureThree />
  <FeatureOne />
  <FeatureFour />
  <PricingOne />
  <FaqOne />
  <CtaOne />
</BaseLayout>